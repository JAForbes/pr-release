    "version": "0.2.0",
    "configurations": [
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "pr",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "args": ["pr"],
            "program": "${workspaceFolder}/lib/index.js"
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "changelog",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "args": ["changelog"],
            "program": "${workspaceFolder}/lib/index.js"
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "extract-changelog",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "args": ["extract-changelog", "--out"],
            "program": "${workspaceFolder}/lib/index.js"
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "merge --commit",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "args": ["merge", "--commit"],
            "program": "${workspaceFolder}/lib/index.js"
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "actions-yml",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "args": ["actions-yml"],
            "program": "${workspaceFolder}/lib/index.js"
        }
    ]
}